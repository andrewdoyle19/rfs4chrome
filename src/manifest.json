{
  "manifest_version": 2,

  "name": "RFS4Chrome",
  "description": "This extension reads the NSW Rural Fire Service XML Document",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

"content_security_policy": "script-src 'self' https://rfsforchromeservice.apphb.com http://localhost; object-src 'self'",

"permissions": [
  	"http://rfsforchromeservice.apphb.com/*",
  	"http://localhost/*"
  ]
}